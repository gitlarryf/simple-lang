            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">CASE&#x000D;&#x000A;CONST&#x000D;&#x000A;DECLARE&#x000D;&#x000A;DO&#x000D;&#x000A;ELSE&#x000D;&#x000A;ELSEIF&#x000D;&#x000A;END&#x000D;&#x000A;ENUM&#x000D;&#x000A;EXIT&#x000D;&#x000A;EXTERNAL&#x000D;&#x000A;FOR&#x000D;&#x000A;FUNCTION&#x000D;&#x000A;IF&#x000D;&#x000A;IMPORT&#x000D;&#x000A;LOOP&#x000D;&#x000A;NEW&#x000D;&#x000A;NEXT&#x000D;&#x000A;POINTER&#x000D;&#x000A;RECORD&#x000D;&#x000A;REPEAT&#x000D;&#x000A;RETURN&#x000D;&#x000A;STEP&#x000D;&#x000A;THEN&#x000D;&#x000A;TO&#x000D;&#x000A;TYPE&#x000D;&#x000A;UNTIL&#x000D;&#x000A;VALID&#x000D;&#x000A;VAR&#x000D;&#x000A;WHEN&#x000D;&#x000A;WHILE&#x000D;&#x000A;EXCEPTION&#x000D;&#x000A;TRY&#x000D;&#x000A;</Keywords>
            <Keywords name="Keywords2">RAISE&#x000D;&#x000A;AND&#x000D;&#x000A;NOT&#x000D;&#x000A;OR</Keywords>
